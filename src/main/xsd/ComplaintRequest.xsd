<xsd:schema version="1.0"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            elementFormDefault="qualified"
                 
            targetNamespace="http://localhost:8084/complaints/ComplaintService/schemas">
    
    
    <xsd:element name="ComplaintRequest"> 
        <xsd:complexType>
            <xsd:sequence> 
                <xsd:element name="company" type="xsd:string"/>
                <xsd:element name="description" type="xsd:string"/>                
            </xsd:sequence>          
        </xsd:complexType>
    </xsd:element>
    
    <xsd:element name="ComplaintResponse"> 
        <xsd:complexType>
            <xsd:sequence> 
                <xsd:element name="status" type="xsd:string"/>
               
            </xsd:sequence>          
        </xsd:complexType>
    </xsd:element>
       
</xsd:schema>
